<head>
<%= javascript_include_tag 'jquery.hashchange.js' %>
<%= javascript_include_tag 'jquery.easytabs.min.js' %>

<script type="text/javascript">
  $(document).ready(function(){
    $('#tab-container').easytabs();
  });
</script>


</head>

<h1>Current Rosters </h1>

<div id="tab-container" class="prepend-2 span-20 append-2 last">


  <ul>
    <li><a href="#a-team-div">A Team</a></li>
    <li><a href="#b-team-div">B Team</a></li>
    <li><a href="#full-roster-div">Full Roster</a></li>
    <li><a href="#legends-div">Legends</a></li>
  </ul>

  <div id="a-team-div">
    <%= render 'playerTable' %>
    <%= render 'aTeam' %>
    </table>
  </div>
    <div id="b-team-div">
    <%= render 'playerTable' %>
    <%= render 'bTeam' %>
    </table>
  </div>
  <div id="full-roster-div">
    <%= render 'playerTable' %>
    <%= render 'fullRoster' %>
    </table>
  </div>
  <div id="legends-div">
    <%= render 'playerTable' %>
    <%= render 'legends' %>
    </table>
  </div>


</div>

